<h1>Player: <%= @player.name %></h1>
<h3>Player: <%= @player.player_number %></h3>
<h3>Starter?: <%= @player.starter ? "Yes" : "No" %></h3>

<h3>Match:</h3>
<%= @player.matches.each do |match| %>
<p><%= match.valid_match ? "Yes" : "No" %></p>
<%end%>

<div>
<br>
<%= link_to "Edit This Match", edit_user_player_path %>
</br>
<br>
<%= link_to "Go To Players", user_players_path %>
</br>
<br>
<%= link_to "Go To Welcome Page", user_session_path %>
</br>
<br>
<%= button_to "Delete This Match", user_player_path(@player), method: :delete %>
</br>
</div>