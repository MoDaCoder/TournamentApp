<%= form_for (@player) do |f| %>
    <h2>Players:</h2>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :player_number %>
    <%= f.number_field :player_number%>
<%= f.fields_for :user do |ff| %>
    <%= ff.label :user_name %>
    <%= ff.text_field :name %>

<%end%>
<%= f.submit %>
<%end%>

<br>
<%= link_to "Go To Players", user_players_path %>
</br>
<br>
<%= link_to "Welcome Page", user_session_path %>
</br>